<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0</TargetFrameworks>
		<Authors>Daniel Bramer</Authors>
		<OutputPath>$(SolutionDir)$(Configuration)</OutputPath>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>..\..\certificate\ConsoleToolkit.Core.snk</AssemblyOriginatorKeyFile>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Proto\GreeterService.proto" />
	  <None Remove="Proto\UnitTestService.proto" />
	</ItemGroup>


	<ItemGroup>
		<PackageReference Include="FluentAssertions" />
		<PackageReference Include="FluentSetups" />
		<PackageReference Include="MSTest.TestAdapter" />
		<PackageReference Include="MSTest.TestFramework" />
		<PackageReference Include="Microsoft.NET.Test.Sdk" />
		<PackageReference Include="Moq" />
		<PackageReference Include="Google.Protobuf" />
		<PackageReference Include="Grpc.Tools">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>



	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>
				DynamicProxyGenAssembly2, 
			</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>



	<ItemGroup>
		<ProjectReference Include="..\ConsoLovers.Ipc.Client\ConsoLovers.Ipc.Client.csproj" />
		<ProjectReference Include="..\ConsoLovers.Ipc.Server\ConsoLovers.Ipc.Server.csproj" />
	</ItemGroup>



	<ItemGroup>
	  <Protobuf Include="Proto\GreeterService.proto" Access="Internal" GrpcServices="Both" />
	  <Protobuf Include="Proto\UnitTestService.proto">
	    <Access>Internal</Access>
	    <GrpcServices>Both</GrpcServices>
	  </Protobuf>
	</ItemGroup>

</Project>
